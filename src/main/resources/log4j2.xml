<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%highlight{%d [%t] %-5level: %msg%n%throwable}"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level= "warn">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>